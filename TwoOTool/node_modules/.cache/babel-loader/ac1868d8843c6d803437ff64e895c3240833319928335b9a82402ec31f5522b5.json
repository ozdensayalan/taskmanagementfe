{"ast":null,"code":"import TeamService from '../services/TeamService';\nimport EmployeeEditPopup from '../components/EmployeeEditPopup.vue';\nimport EmployeePopUp from '../components/EmployeePopUp.vue';\nimport store from '../js/store';\nimport axios from 'axios';\nexport default {\n  emits: ['editMember'],\n  components: {\n    EmployeeEditPopup,\n    EmployeePopUp\n  },\n  name: 'EmployeeList',\n  data() {\n    return {\n      employees: []\n    };\n  },\n  watch: {\n    employees: {\n      deep: true,\n      handler() {\n        this.getEmployees();\n      }\n    }\n  },\n  computed: {\n    isAdmin() {\n      return store.state.admin === 'yes';\n    }\n  },\n  methods: {\n    getEmployees() {\n      TeamService.getEmployees().then(response => {\n        this.employees = response.data;\n      });\n    },\n    deleteRecord(id) {\n      axios.delete(`http://localhost:8080/deleteUser?id=${id}`).then(response => {\n        console.log('Kullanıcı başarıyla silindi:', response);\n      }).catch(error => {\n        console.error('Kullanıcı silme hatası:', error);\n      });\n    }\n  },\n  created() {\n    this.getEmployees();\n  }\n};","map":{"version":3,"names":["TeamService","EmployeeEditPopup","EmployeePopUp","store","axios","emits","components","name","data","employees","watch","deep","handler","getEmployees","computed","isAdmin","state","admin","methods","then","response","deleteRecord","id","delete","console","log","catch","error","created"],"sources":["src/pages/TheTeam.vue"],"sourcesContent":["<template >\n    <div>\n        <h1 class=\"grey--text mb-3\">Team Members</h1>\n        <v-layout row wrap>\n            <v-flex xs12 sm6 md4 lg3 v-for=\"person in employees\" :key=\"person.id\"  @mouseover=\"hoveredIndex = person.id\" @mouseleave=\"hoveredIndex = null\">\n                <v-card class=\"ma-4\">\n        <v-tooltip bottom color=\"black\" text-color=\"blue\" border-radius=\"8px\" elevation=\"10\">\n        <template v-slot:activator=\"{ on }\">\n          <div @mouseover=\"hoveredIndex = index\" @mouseleave=\"hoveredIndex = null\" v-on=\"on\">\n            <div class=\"d-flex justify-center pt-4 pb-2\">\n              <v-avatar size=\"100\">\n                <img :src=\"person.avatar\">\n              </v-avatar>\n            </div>\n            <div class=\"d-flex justify-center\">\n              <v-card-title class=\"pa-0\">{{ person.name }}</v-card-title>\n            </div>\n          </div>\n          <div class=\"d-flex justify-end mb-2\"  v-if=\"isAdmin\">\n                        <div class=\"mr-4\"> \n                             <employee-edit-popup :initialContent=\"person\" @editMember=\"editMember\"></employee-edit-popup>\n                             <v-btn style=\"width: 30px; height: 15px;  background-color: #0074D9;\" v-if=\"isAdmin\" @click=\"deleteRecord(person.id)\">            \n                              <span style=\"font-size: 10px; color: white;\">Delete</span>\n                            </v-btn>\n                        </div>\n                    </div>\n        </template>\n        <span>{{ person.title }}</span><br>\n        <span>{{ person.email }}</span>\n      </v-tooltip>\n                </v-card>\n            </v-flex>\n        </v-layout>\n        <div class=\"employee-popup-container\" v-if=\"isAdmin\">\n                        <div class=\"mr-4\"> \n                             <employee-pop-up @editMember=\"editMember\"></employee-pop-up>\n                        </div>\n                    </div>\n                    <div>\n  </div>\n    </div>\n</template>\n\n<script>\nimport TeamService from '../services/TeamService';\nimport EmployeeEditPopup from '../components/EmployeeEditPopup.vue';\nimport EmployeePopUp from '../components/EmployeePopUp.vue';\nimport store from '../js/store'; \nimport axios from 'axios';\n\n    export default{\n        emits: ['editMember'],\n        components: {\n        EmployeeEditPopup,\n        EmployeePopUp\n    },\n        name: 'EmployeeList',\n        data(){\n            return{\n                employees : []            }\n        },   \n        watch: {\n            employees: {\n      deep: true,\n      handler() {\n        this.getEmployees();\n      }\n    }\n  },    \n  computed: {\n    isAdmin() {\n      return store.state.admin==='yes';\n    }},\n        methods: {\n            getEmployees(){\n                TeamService.getEmployees().then((response)=>{\n                    this.employees=response.data;\n                });\n            },\n        deleteRecord(id) {\n      axios.delete(`http://localhost:8080/deleteUser?id=${id}`)\n        .then(response => {\n          console.log('Kullanıcı başarıyla silindi:', response);\n        })\n        .catch(error => {\n          console.error('Kullanıcı silme hatası:', error);\n        });\n    }\n        },\n        created() {\n            this.getEmployees();\n        }\n    }\n</script>\n<style>\n.employee-popup-container {\n  position: fixed;\n  bottom: 20px;\n  right: 20px; \n}\n.custom-popup {\n  width: 300px;\n  height: 400px;\n}\n</style>"],"mappings":"AA4CA,OAAAA,WAAA;AACA,OAAAC,iBAAA;AACA,OAAAC,aAAA;AACA,OAAAC,KAAA;AACA,OAAAC,KAAA;AAEA;EACAC,KAAA;EACAC,UAAA;IACAL,iBAAA;IACAC;EACA;EACAK,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;IAAA;EACA;EACAC,KAAA;IACAD,SAAA;MACAE,IAAA;MACAC,QAAA;QACA,KAAAC,YAAA;MACA;IACA;EACA;EACAC,QAAA;IACAC,QAAA;MACA,OAAAZ,KAAA,CAAAa,KAAA,CAAAC,KAAA;IACA;EAAA;EACAC,OAAA;IACAL,aAAA;MACAb,WAAA,CAAAa,YAAA,GAAAM,IAAA,CAAAC,QAAA;QACA,KAAAX,SAAA,GAAAW,QAAA,CAAAZ,IAAA;MACA;IACA;IACAa,aAAAC,EAAA;MACAlB,KAAA,CAAAmB,MAAA,wCAAAD,EAAA,IACAH,IAAA,CAAAC,QAAA;QACAI,OAAA,CAAAC,GAAA,iCAAAL,QAAA;MACA,GACAM,KAAA,CAAAC,KAAA;QACAH,OAAA,CAAAG,KAAA,4BAAAA,KAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAf,YAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}