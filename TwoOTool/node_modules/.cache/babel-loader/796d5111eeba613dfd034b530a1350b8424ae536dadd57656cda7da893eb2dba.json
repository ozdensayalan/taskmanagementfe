{"ast":null,"code":"import store from '../js/store';\nimport axios from 'axios';\nimport PopUp from '../components/PopUp.vue';\nexport default {\n  emits: ['addedProject'],\n  components: {\n    PopUp\n  },\n  data() {\n    return {\n      snackbar: false,\n      title: '',\n      drawer: false,\n      projects: []\n    };\n  },\n  created() {\n    this.getTasks();\n  },\n  methods: {\n    sortBy(prop) {\n      this.projects.sort((a, b) => a[prop] < b[prop] ? -1 : 1);\n    },\n    showSnackBar(title) {\n      this.title = title;\n      this.snackbar = true;\n    },\n    async getTasks() {\n      const tasks = {\n        id: store.state.id\n      };\n      try {\n        const response = await axios.get('http://localhost:8080/taskById', {\n          params: tasks\n        });\n        this.projects = response.data;\n      } catch (error) {\n        console.error(error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["store","axios","PopUp","emits","components","data","snackbar","title","drawer","projects","created","getTasks","methods","sortBy","prop","sort","a","b","showSnackBar","tasks","id","state","response","get","params","error","console"],"sources":["src/pages/DashBoard.vue"],"sourcesContent":["<template>\n    <div>\n        <h1 class=\"grey--text ml-3 mb-4\">Dashboard</h1>\n        <v-snackbar v-model=\"snackbar\" color=\"indigo\">\n            <div class=\"d-flex justify-space-between\">\n                <span>Project {{ title }} is added sucessfully</span>\n                <v-icon color=\"white\">mdi-check-circle</v-icon>\n            </div>\n        </v-snackbar>\n            <div class=\"mr-4\"> \n                <pop-up @addedProject=\"showSnackBar\"></pop-up>\n            </div>\n        <v-layout class=\"ml-3\">\n            <div class=\"float-left\">\n                <v-btn depressed class=\"mb-4 mr-3\" @click=\"sortBy('title')\">\n                    <span>Sort</span>\n                    <v-icon right>mdi-message</v-icon>\n                </v-btn>\n                <v-btn depressed class=\"mb-4\" @click=\"sortBy('assing')\">\n                    <span>Sort</span>\n                    <v-icon right>mdi-account</v-icon>\n                </v-btn>\n            </div>\n        </v-layout>\n        <v-container>\n            <v-card v-for=\"project in projects\" :key=\"project.id\" :class=\"`project ${project.report}`\">\n                <v-layout row wrap class=\"mx-4 mb-8 pa-4\">\n                    <v-flex xs6 md4 class=\"pb-1\">\n                        <div class=\"caption grey--text\">Task Title</div>\n                        <div>{{ project.title }}</div>\n                    </v-flex>\n                    <v-flex xs6 sm4 md2 class=\"pb-1\">\n                        <div class=\"caption grey--text\">Assing To</div>\n                        <div>{{ project.assign }}</div>\n                    </v-flex>\n                    <v-flex xs6 sm4 md2 class=\"pb-1\">\n                        <div class=\"caption grey--text\">Status</div>\n                        <div>{{ project.status }}</div>\n                    </v-flex>\n                    <v-flex xs6 sm4 md2 class=\"pb-1\">\n                        <div class=\"caption grey--text\">Due Date</div>\n                        <div>{{ project.dueDate }}</div>\n                    </v-flex>\n                    <v-flex xs6 sm4 md2 class=\"pb-1 d-flex align-center justify-sm-end\">\n                        <v-chip :class=\"`white--text ${project.report}`\">{{ project.report }}</v-chip>\n                    </v-flex>\n                </v-layout>\n            </v-card>\n        </v-container>\n    </div>\n</template>\n\n<script>\n\nimport store from '../js/store'; \nimport axios from 'axios';\nimport PopUp from '../components/PopUp.vue';\n\nexport default {\n    emits: ['addedProject'],\n    components: {\n        PopUp\n    },\n    data() {\n        return {\n            snackbar: false,\n            title: '',\n            drawer: false,\n            projects: []\n        }\n    },\n    created() {\n        this.getTasks();\n    },\n    methods: {\n        sortBy(prop) {\n            this.projects.sort((a,b) => a[prop] < b[prop]? -1 : 1);\n        },   \n        showSnackBar(title) {\n            this.title = title;\n            this.snackbar = true;\n        },    \n        async getTasks(){\n            const tasks = {\n        id: store.state.id\n      };\n        try {\n            const response = await axios.get('http://localhost:8080/taskById', {\n            params: tasks\n        });\n            this.projects=response.data\n        \n      } catch (error) {\n        console.error(error);\n      }\n    }\n  }\n}\n</script>\n\n<style>\n.project.completed {\n    border-left: 4px solid turquoise;\n}\n\n.project.ongoing {\n    border-left: 4px solid orange;\n}\n\n.project.overdue {\n    border-left: 4px solid tomato;\n}\n\n.v-chip.completed {\n    background: turquoise !important;\n}\n.v-chip.ongoing {\n    background: orange !important;\n}\n.v-chip.overdue {\n    background: tomato !important;\n}\n</style>"],"mappings":"AAsDA,OAAAA,KAAA;AACA,OAAAC,KAAA;AACA,OAAAC,KAAA;AAEA;EACAC,KAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACA;MACAC,QAAA;MACAC,KAAA;MACAC,MAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACAC,OAAAC,IAAA;MACA,KAAAL,QAAA,CAAAM,IAAA,EAAAC,CAAA,EAAAC,CAAA,KAAAD,CAAA,CAAAF,IAAA,IAAAG,CAAA,CAAAH,IAAA;IACA;IACAI,aAAAX,KAAA;MACA,KAAAA,KAAA,GAAAA,KAAA;MACA,KAAAD,QAAA;IACA;IACA,MAAAK,SAAA;MACA,MAAAQ,KAAA;QACAC,EAAA,EAAApB,KAAA,CAAAqB,KAAA,CAAAD;MACA;MACA;QACA,MAAAE,QAAA,SAAArB,KAAA,CAAAsB,GAAA;UACAC,MAAA,EAAAL;QACA;QACA,KAAAV,QAAA,GAAAa,QAAA,CAAAjB,IAAA;MAEA,SAAAoB,KAAA;QACAC,OAAA,CAAAD,KAAA,CAAAA,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}