flex justify-space-between\">\n                <span>Project {{ title }} is added sucessfully</span>\n                <v-icon color=\"white\">mdi-check-circle</v-icon>\n            </div>\n        </v-snackbar>\n        <v-app-bar flat app>\n            <v-app-bar-nav-icon class=\"grey--text\" @click=\"drawer = !drawer\"></v-app-bar-nav-icon>\n            <v-app-bar-title class=\"text-uppercase grey--text\">\n                <div class=\"d-flex\">\n                    <img src=\"/favicon.svg\" height=\"30px\">\n                    <span class=\"font-weight-light\">TASK</span>\n                    <span>MANAGE</span>\n                </div>\n            </v-app-bar-title>\n            <v-spacer></v-spacer>\n            <v-btn text @click=\"logout\">\n                <span>Sign out</span>\n                <v-icon right>mdi-logout</v-icon>\n            </v-btn>\n        </v-app-bar>\n\n        <v-navigation-drawer v-model=\"drawer\" app class=\"blue\">\n            <div class=\"mt-5 d-flex justify-center\">\n                <v-avatar size=\"160\">\n                    <img :src=\"avatar\">\n                </v-avatar>\n            </div>\n            <div>\n                <h2 class=\"white--text text-center my-3\">{{ username }}</h2>\n            </div>\n            <div class=\"d-flex justify-center mb-2\">\n                <pop-up :initialContent=\"project\" @addedProject=\"showSnackBar\" @click=\"addToStore\"></pop-up>\n            </div>\n            <v-list>\n                <v-list-item v-for=\"link in links\" :key=\"link.name\" :to=\"link.route\">\n                    <v-list-item-action>\n                        <v-icon class=\"white--text\">{{ link.icon }}</v-icon>\n                    </v-list-item-action>\n                    <v-list-item-content>\n                        <v-list-item-title class=\"white--text\">{{ link.name }}</v-list-item-title>\n                    </v-list-item-content>\n                </v-list-item>\n            </v-list>\n        </v